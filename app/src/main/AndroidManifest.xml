<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sugoi.android.amazfun">


    <application android:theme="@style/AppTheme" android:supportsRtl="true" android:label="@string/app_name" android:icon="@mipmap/ic_launcher" android:allowBackup="true" android:name="sugoi.android.amazfun.FirebaseChatMainApp">


    <activity android:theme="@style/AppTheme.NoActionBar.Splash" android:label="@string/app_name" android:name="sugoi.android.amazfun.ui.activities.SplashActivity">


    <intent-filter>

    <action android:name="android.intent.action.MAIN"/>

    <category android:name="android.intent.category.LAUNCHER"/>

</intent-filter>

</activity>

    <activity android:theme="@style/AppTheme.NoActionBar" android:label="@string/app_name" android:name="sugoi.android.amazfun.ui.activities.LoginActivity"/>

    <activity android:theme="@style/AppTheme.NoActionBar" android:label="@string/app_name" android:name="sugoi.android.amazfun.ui.activities.RegisterActivity"/>

    <activity android:theme="@style/AppTheme.NoActionBar" android:label="@string/app_name" android:name="sugoi.android.amazfun.ui.activities.UserListingActivity"/>

    <activity android:theme="@style/AppTheme.NoActionBar" android:label="@string/app_name" android:name="sugoi.android.amazfun.ui.activities.ChatActivity"/>


    -<service android:name="sugoi.android.amazfun.fcm.MyFirebaseMessagingService">


    -<intent-filter>

    <action android:name="com.google.firebase.MESSAGING_EVENT"/>

</intent-filter>

</service>


    <service android:name="sugoi.android.amazfun.fcm.MyFirebaseInstanceIDService">


    <intent-filter>

    <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>

</intent-filter>

</service>

</application>

</manifest>